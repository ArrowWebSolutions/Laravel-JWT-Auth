{
    "name": "arrow-web-sol/laravel-jwt-auth",
    "description": "Authenticate users via JWT in Laravel.",
    "keywords": [
        "Arrow Web Solutions",
        "JWT",
        "Authentication",
        "Laravel"
    ],
    "homepage": "https://github.com/ArrowWebSolutions/Laravel-JWT-Auth",
    "require": {
        "php": "8.0.*|8.1.*|8.2.*",
        "ramsey/uuid": "^4.7",
        "lcobucci/jwt": "^4.3",
        "illuminate/auth": "^9.0|^10.0|^11.0",
        "illuminate/contracts": "^9.0|^10.0|^11.0",
        "illuminate/http": "^9.0|^10.0|^11.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "spatie/laravel-package-tools": "^1.13",
        "nesbot/carbon": "^2.64"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0|^7.0|^8.0",
        "larastan/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "pestphp/pest": "^1.21|^2.0",
        "pestphp/pest-plugin-laravel": "^1.1|^2.0",
        "phpseclib/phpseclib": "^3.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5|^10.0|^11.0",
        "spatie/laravel-ray": "^1.26"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Arron King",
            "email": "arron.king@arrow-web.dev",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Arrow\\JwtAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arrow\\JwtAuth\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arrow\\JwtAuth\\JwtAuthenticationServiceProvider"
            ]
        }
    }
}
